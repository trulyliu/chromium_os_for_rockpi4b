+/dts-v1/;
+
+/ {
+	compatible = "rockchip,rockpi", "rockchip,rk3399";
+	interrupt-parent = <0x1>;
+	#address-cells = <0x2>;
+	#size-cells = <0x2>;
+	model = "ROCK PI 4B";
+
+	ddr_timing: ddr_timing {
+		compatible = "rockchip,ddr-timing";
